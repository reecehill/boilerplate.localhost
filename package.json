{
  "name": "admin-boilerplate",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "build-docs": "npx typedoc",
    "clean": "npm run clean:dist:win && npm run clean:dist:lin && npm run clean:dts:win && npm run clean:dts:lin",
    "clean:dist:win": "node -e \"if (process.platform === 'win32') process.exit(1)\" || , if exist dist rmdir /Q /S dist",
    "clean:dist:lin": "node -e \"if (process.platform !== 'win32') process.exit(1)\" || rm -rf dist",
    "clean:dts:win": "node -e \"if (process.platform === 'win32') process.exit(1)\" || , if exist dts rmdir /Q /S dts",
    "clean:dts:lin": "node -e \"if (process.platform !== 'win32') process.exit(1)\" || rm -rf dts"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.34.2",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.8",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "cssnano": "^7.0.1",
    "cypress": "^13.8.1",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^2.15.2",
    "eslint-plugin-vue": "^9.25.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "sass": "^1.76.0",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.4.3",
    "typedoc": "^0.25.13",
    "typedoc-plugin-missing-exports": "^2.2.0",
    "typedoc-plugin-vue": "^1.1.0",
    "typescript": "~5.4.0",
    "vite": "^5.2.11",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.16"
  }
}
